{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "lint-css": "stylelint -s scss src",
    "lint-js": "eslint --ext .jsx --ext .js src",
    "lint": "yarn run lint-js && yarn run lint-css",
    "test": "yarn run lint ",
    "cypress": "cypress open "
  },
  "dependencies": {
    "@contentful/gatsby-transformer-contentful-richtext": "^13.1.0",
    "cypress": "^3.4.1",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-react": "^7.14.2",
    "gatsby": "^2.13.50",
    "gatsby-plugin-postcss": "^2.0.0",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-typography": "^2.3.2",
    "gatsby-remark-images": "^3.1.7",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-contentful": "^2.1.67",
    "gatsby-source-filesystem": "^2.1.8",
    "gatsby-transformer-remark": "^2.6.10",
    "gatsby-transformer-sharp": "^2.3.7",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.0",
    "postcss-preset-env": "^6.0.10",
    "postcss-reporter": "^6.0.0",
    "react": "^16.8.6",
    "react-css-modules": "^4.7.1",
    "react-dom": "^16.8.6",
    "react-icons": "2.2.7",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "gatsby-cypress": "^0.2.8",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.10.2",
    "stylelint": "^9.6.0",
    "stylelint-order": "1.0.0"
  }
}
